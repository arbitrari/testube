{
  "name": "testube",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node version.js && ng serve",
    "build": "node version.js && ng build --configuration production --build-optimizer --output-hashing=all",
    "publish": "npm run build && ngh --repo=https://GH_TOKEN@github.com/arbitrarydot/testube.git --name=\"JaxonWright\" --email=jaxonhwright@gmail.com --no-silent",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "inspect": "ng build --configuration production --build-optimizer --output-hashing=all --source-map && npx source-map-explorer dist/**/*.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.5",
    "@angular/cdk": "~13.3.5",
